{
    "name": "SeeSynesthete",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "run_at": "document_idle",
            "js": ["content.js", "pageProcessor.js"]  
        }
    ],
    "browser_action": {
        "default_icon": {                             
        },
        "default_title": "Settings",     
        "default_popup": "popup.html"        
      } 
}